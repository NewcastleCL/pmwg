Package: pmwg
Title: Particle Metropolis Within Gibbs
Version: 0.1.8
Authors@R: c(
	person("Gavin", "Cooper", email = "gavin@gavincooper.net", role = c("aut", "cre", "trl"), comment = "Package creator, additional translation of MATLAB code"),
